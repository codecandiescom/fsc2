DEVICES:

tds520;


VARIABLES:

curve[ 2, * ];
I, J;


PREPARATIONS:

init_2d( 1, 0, 0, 0, 0.4 ns, 0, 1, "time [ns]",
		 "BLA", "Intensity (arbitrary units)" );

EXPERIMENT:

FOR I = 1 : 200 {
	digitizer_start_acquisition( );
	curve[ 1 ] = digitizer_get_curve( CH1 );
    display( 1, I, curve[ 1 ], 1 );
}