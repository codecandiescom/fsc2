/***********************************************************************/
/* This is the device name data base used by fsc2. To add a new device */
/* just append its name to this file - the names are case-insensitive  */
/* (i.e. they are automatically converted to all lower case).          */
/* If the device is controlled via the GPIB bus it also has to be      */
/* listed in `/etc/gpib.conf'!                                         */
/* C and C++ style comments can be used within this file.              */
/***********************************************************************/


// Pulser

dg2020_f      // Sony/Tektronix data generator (Frankfurt S-band)
dg2020_b      // Sony/Tektronix data generator (Berlin X-band)
hfs9000       // Tektronix stimulus system     (Berlin W-band)
ep385         // Bruker Pulse Programmer       (Berlin X-band)
rs690         // Interface Technology RS-690 Word Generator (Berlin 360 GHz)
rb_pulser     // Leiden pulser made up from Rulbus cards


// Digital oszilloscopes

tds520        // Tektronix digitizing oscilloscope 520
tds520a       // Tektronix digitizing oscilloscope 520A
tds520c       // Tektronix digitizing oscilloscope 520C
tds540        // Tektronix digitizing oscilloscope 540
tds744a       // Tektronix digitizing oscilloscope 744A
tds754a       // Tektronix digitizing oscilloscope 754A
lecroy9400    // LeCroy digitizing oscilloscope 9400
lecroy9450    // LeCroy digitizing oscilloscope 9450
lecroy9424    // LeCroy digitizing oscilloscope 9424
lecroy9424e   // LeCroy digitizing oscilloscope 9424e
lecroy_wr2    // LeCroy digitizing oscilloscope Waverunner 2


// Lock-In amplifiers

sr510         // Stanford Research lock-in amplifier 510
sr530         // Stanford Research lock-in amplifier 530
sr810         // Stanford Research lock-in amplifier 810
sr830         // Stanford Research lock-in amplifier 830
er023m        // Bruker Signal Channel ER 023 M


// Magnet power suplies

aeg_s_band    // S-band AEG magnet power supply (Frankfurt)
aeg_x_band    // X-band AEG magnet power supply (Berlin)
keithley228a  // W-band magnet power supply (Berlin)
er032m        // Bruker ER 032 M field controller
ips20_4       // Oxford sweep power supply
ips120_10     // Oxford sweep power supply
s_band_magnet_broad;    // special module for "broken" Frankfurt S-band
s_band_magnet_narrow;   // special module for "broken" Frankfurt S-band
hjs_sfc;      // simple field control for Steinhoff group
hjs_fc;       // field control with field checking for Steinhoff group


// Field meter

er035m        // Bruker NMR gaussmeter (connected to IEEE bus)
er035m_s      // Bruker NMR gaussmeter (connected to serial port)
er035m_sa     // Bruker NMR gaussmeter (connected to IEEE bus, stand alone)
er035m_sas    // Bruker NMR gaussmeter (connected to serial port, stand alone)
bh15          // Bruker Hall effect field controller
pt2025        // Metrolab NMR teslameter
bnm12         // Bruker NMR gaussmeter (requires WITIO-48 card)


// Synthesizers

hp8647a       // HP RF synthesizer
hp8648b       // HP RF synthesizer
hp8672a       // HP RF synthesizer
rs_sml01      // Rohde&Schwarz SML01 Signal Generator


// Box-car integrators

egg4402       // EG&G PARC box-car


// Multimeters

kontron4060   // Kontron 4060 voltmeter
schlum7150    // Schlumberger 7150

// Temperature controllers

lakeshore330  // Lakeshore 330 temperature controller
itc503;       // Oxford ITC503 temperature controller


// Frequency counter

hp5340a       // HP5340A frequency counter


// DAQ cards and devices (ME-6000 board, HJS_DAADC, NI PCI-MIO-16E-1,
// Rulbus ADC and DAC cards)

me6000        // ME-6000 DAC card
hjs_daadc     // DA/AD converter from the group of H.-J. Steinhoff (Osnabrueck)
pci_mio_16e_1 // PCI-MIO-16E-1 DAQ card by National Instruments
rb8509        // Rulbus 12-bit ADC card
rb8510_0      // Rulbus 12-bit DAC card
rb8510_1      // Rulbus 12-bit DAC card


// Event counter (NI6601 board)

ni6601


// CCD cameras (Roper Scientific Spec-10)

rs_spec10


// Monochromators (Acton SpectaPro-300i)

spectrapro_300i
spex_cd2a


// Power Supplies (Thurlby&Thandar PL330DP, F.u.G. MCN700-2000)

thurlby330
mcn700_2000


// DIOs (WITIO-48 card)

witio_48


// Microwave attenuators 

hjs_attenuator           // stepper motor controlled attenuator from the
						 // group of H.-J. Steinhoff (Osnabrueck)


// Choppers

gg_chopper
